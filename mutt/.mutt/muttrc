set quit=yes
set edit_headers=yes
set fast_reply=yes
hdr_order date from subject to cc reply-to
ignore *
unignore from: date subject to cc reply-to
unignore Organization sender: Mail-Followup-To
unignore Posted-To: X-Mailer: User-Agent: X-Newsreader:
unignore resent- x-sender: x-spam-status:
unignore List-
set hdrs=yes
set postpone=yes
set recall=no
set abort_nosubject=no
set alias_file=~/.mutt/aliases
set followup_to=yes
set honor_followup_to=yes
set ignore_list_reply_to=yes
set dsn_notify=never
set abort_unmodified=no
set delete=yes
set confirmappend=no
set include=yes
set autoedit=no
set attribution="On %d, %f wrote:\n"
set date_format="%A, %e %B %Y, %l:%M %P %Z"
set help=no
set mark_old=no
set move=no
set wait_key=no
set reverse_name=no
set reverse_realname=no
set use_from=yes
set hidden_host=yes
set ascii_chars=yes
set beep_new=yes
set envelope_from=yes
set metoo=no
